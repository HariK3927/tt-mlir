include "types.fbs";

namespace tt;

enum CoreType : ushort {
  BRISC = 0,
  NCRISC = 1,
  TRISC0 = 2,
  TRISC1 = 3,
  TRISC2 = 4,
  ERISC = 5,
}

table Source {
  source: string;
}

table Binary {
  data: [ubyte];
}

union KernelBinary {
  Source,
  Binary,
}

table Kernel {
  name: string;
  core_type: CoreType;
  binary: KernelBinary;
}

table Program {
  name: string;
  kernels: [Kernel];
  core_ranges: [Dim2dRange];
}
